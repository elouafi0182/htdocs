            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarText">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item">
                    <a class="nav-link" href=" {{ path('default') }} ">Home
                        <span class="sr-only">(current)</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href=" {{ path('see_rooms') }} ">See Rooms
                        <span class="sr-only"></span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href=" {{ path('book_room') }} ">Book
                        <span class="sr-only"></span>
                    </a>
                </li>
                {% if is_granted('ROLE_SUPER_ADMIN') %}
                    <li class="nav-item dropdown">
                        <a aria-expanded="false" aria-haspopup="true" class="nav-link dropdown-toggle" data-toggle="dropdown" href="#" id="dropdown05">CRUD'S</a>
                        <div aria-labelledby="dropdown05" class="dropdown-menu">
                           
                            <a class="dropdown-item" href="{{ path('betaal_index') }}">Betaal</a>
                            <a class="dropdown-item" href="{{ path('extras_index') }}">Extras</a>
                            <a class="dropdown-item" href="{{ path('image_index') }}">Image</a>
                            <a class="dropdown-item" href="{{ path('organisation_index') }}">Organisation</a>
                            <a class="dropdown-item" href="{{ path('reserveringen_index') }}">Reseveringen</a>
                            <a class="dropdown-item" href="{{ path('soort_index') }}">Soort</a>
                            <a class="dropdown-item" href="{{ path('user_index') }}">Users</a>
                            <a class="dropdown-item" href="{{ path('kamer_index') }}">Kamer</a>
                        </div>
                    </li>
                {% endif %}
            </ul>
            <span class="navbar-text white-text">
                <ul class="navbar-nav ml-auto">
                    {% if app.user %}
                        <li class="nav-item">
                            <a class="nav-link" href="{{ path("myBookings") }}">My Bookings</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="{{ path("fos_user_profile_show") }}">Profile</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="{{ path("fos_user_security_logout") }}">Logout
                                {{ app.user }}</a>
                        </li>
                    {% else %}
                        <li class="nav-item">
                            <a class="nav-link" href="{{ path("fos_user_registration_register") }}">Register</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="{{ path("fos_user_security_login") }}">Login</a>
                        </li>
                    {% endif %}
                </ul>
            </span>
        </div>
</nav>
